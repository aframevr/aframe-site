{
  "name": "aframe-site",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "aframe": "github:aframevr/aframe#6bdc7f0",
    "chalk": "^1.1.1",
    "cheerio": "^0.20.0",
    "es6-promise": "^3.1.2",
    "glob": "^7.0.3",
    "hexo": "^3.2.0",
    "hexo-all-minifier": "0.0.14",
    "hexo-browsersync": "^0.2.0",
    "hexo-deployer-git": "0.1.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.1.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "github:pashokus/hexo-renderer-marked#0267532",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "js-yaml": "^3.5.5",
    "moment": "^2.12.0",
    "request": "^2.69.0",
    "urljoin.js": "^0.1.0"
  },
  "devDependencies": {
    "hexo-cli": "^1.0.1",
    "multidep": "^2.0.0",
    "yaml-cat": "^0.3.2"
  },
  "hexo": {
    "version": "3.2.2"
  },
  "scripts": {
    "checklinks": "node bin/checkLinks",
    "clean": "npm run generate:config && hexo clean",
    "deploy": "hexo deploy && open 'https://aframe.io/'",
    "dev": "npm run clean && npm run server",
    "generate": "npm run generate:config && hexo generate",
    "generate:config": "yaml-cat '_config.prod.yml' '_config.local.yml' -n -m > _config.yml",
    "generate:watch": "hexo generate --watch",
    "predeploy": "npm run clean",
    "postinstall": "multidep multidep.json && cp _config.local.yml.dist _config.local.yml",
    "release": "npm run syncjs ; npm run deploy",
    "server": "hexo server",
    "start": "npm run dev",
    "test": "npm run checklinks"
  }
}
