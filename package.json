{
  "name": "aframe-site",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "aframe": "github:aframevr/aframe#master",
    "chalk": "^1.1.3",
    "cheerio": "^0.20.0",
    "es6-promise": "^3.2.1",
    "glob": "^7.0.6",
    "hexo": "^3.2.2",
    "hexo-browsersync": "^0.2.0",
    "hexo-deployer-git": "^0.1.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "github:pashokus/hexo-renderer-marked#0267532",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "js-yaml": "^3.6.1",
    "lunr": "mamboer/lunr.js",
    "moment": "^2.14.1",
    "request": "^2.74.0",
    "urljoin.js": "^0.1.0"
  },
  "devDependencies": {
    "hexo-cli": "^1.0.2",
    "multidep": "github:ngokevin/node-multidep#github",
    "yaml-cat": "^0.3.2"
  },
  "hexo": {
    "version": "3.2.2"
  },
  "scripts": {
    "bumpdocs": "npm cache clean aframevr/aframe#docs-v0.3.0 && rm -rf .multidep/aframe-v0.3.0 && npm install aframevr/aframe#master && multidep multidep.json",
    "clean": "hexo clean",
    "deploy": "hexo deploy && open 'https://aframe.io/'",
    "dev": "npm run clean && npm run generate:config && npm run server",
    "generate": "npm run generate:config && hexo generate",
    "generate:config": "yaml-cat '_config.prod.yml' '_config.local.yml' -n -m > _config.yml",
    "predeploy": "cp _config.prod.yml _config.yml && npm run clean",
    "postinstall": "multidep multidep.json",
    "release": "npm run syncjs ; npm run deploy",
    "server": "hexo server",
    "start": "npm run dev",
    "test": "node script/checkLinks.js"
  }
}
