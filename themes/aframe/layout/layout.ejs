<% var projectTitle = config.title + ' – ' + config.subtitle %>
<% var isHome = is_current('index.html') %>
<% var showExamples = page.type === 'examples' %>
<!DOCTYPE html>
<html lang="en">
  <head>
    <title><%- page.full_title || (isHome ? projectTitle : (page.title ? page.title + ' — ' : '') + config.title) %></title>
    <%- partial('partials/force_https') %>
    <meta charset="utf-8">
    <meta name="robots" content="<%- config.robots %>">
    <meta name="description" content="<%- projectTitle %>">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="twitter:card" content="A-Frame" />
    <meta name="twitter:site" content="@aframevr" />
    <meta name="twitter:title" content="Building blocks for the virtual reality web" />
    <meta name="twitter:description" content="Use markup to create VR experiences that work across desktop, iPhones, and the Oculus Rift." />
    <meta name="twitter:image" content="https://aframe.io/images/card.png" />
    <meta property="og:title" content="Building blocks for the virtual reality web"/>
    <meta property="og:image" content="https://aframe.io/images/card.png"/>
    <meta property="og:site_name" content="A-Frame"/>
    <meta property="og:description" content="Use markup to create VR experiences that work across desktop, iPhones, and the Oculus Rift."/>
    <link rel="icon" href="<%- url_for('images/aframe-logo.png') %>" type="image/x-icon">
    <% if (isHome) { %>
      <%- js('js/mobile.js') %>
    <% } %>
    <% if (showExamples) { %>
      <%- css('css/examples') %>
    <% } else { %>
      <%- css('css/secondary') %>
    <% } %>
    <%- partial('partials/ga') %>
  </head>
  <body data-page-layout="<%- page.layout %>" data-page-type="<%- page.type %>" data-is-home="<%- isHome %>">
    <% if (!showExamples) { %>
      <%- partial('partials/secondary/mobile_nav') %>
    <% } %>
    <main id="main" class="main">
      <%- body %>
    </main>
    <%- js('js/common.js') %>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fastclick/1.0.6/fastclick.min.js"></script>
    <script>
      (function () {
        if ('FastClick' in window) {
          document.addEventListener('DOMContentLoaded', function () {
            FastClick.attach(document.body);
          });
        }
      })();
    </script>
  </body>
</html>
